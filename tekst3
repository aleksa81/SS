
ORG 0
.text.1
DD 12 DUP ?
end1:

.text.2
b: LOAD R1, b
c: LOAD R1, c

ORG end1
.text.2
DW 48 DUP 4
end2:

ORG end2
.data.1
DD 20 DUP 3
end3:

.rodata
DD 15 DUP ?
x: DD 1 DUP 0
y:
.global y, o, b

ORG end3
.bss
DD ?
end4:

ORG end4
.text
z:ADD R1, R2, R3
k:LOAD R2, $(z+3)
LOAD R1, $(k+1)
LOAD R4, #(x+1)
DD 12 DUP (x-y) + k + b
LOAD R3, #((z-k)+5+y)
JZ R0, $b
JMP R1

.end